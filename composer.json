{
    "name": "mrdejong/themer",
    "description": "Theme package for laravel 5",
    "version": "2.0.0-dev",
    "type": "laravel-package",
    "keywords": [
        "laravel",
        "laravel 5",
        "theme",
        "themes",
        "theming",
        "styling",
        "css preprocessing",
        "asset support"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander de Jong (mrdejong)",
            "email": "mrdejong89@gmail.com"
        }
    ],
    "suggest": {
        "codesleeve/asset-pipeline": "Enables the asset support in themer"
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.2.x"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.3",
        "mockery/mockery": "0.8.0",
        "orchestra/testbench": "2.2.*"
    },
    "autoload": {
        "psr-0": {
            "Mrdejong\\Themer": "src/"
        }
    },
    "minimum-stability": "stable"
}
